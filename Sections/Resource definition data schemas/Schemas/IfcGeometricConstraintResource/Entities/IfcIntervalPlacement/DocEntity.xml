<?xml version="1.0" encoding="utf-8"?>
<DocEntity xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Name="IfcIntervalPlacement" UniqueId="8ea05528-1a9f-41bf-9ac2-47a2b87de488" BaseDefinition="IfcLinearSpanPlacement" EntityFlags="32">
	<Attributes>
