<?xml version="1.0" encoding="utf-8"?>
<DocEntity xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Name="IfcOpenCrossProfileDef" UniqueId="19418b8c-8cd1-4093-ba4b-5f4cf04de71e" BaseDefinition="IfcProfileDef" EntityFlags="32">
	<Attributes>
		<DocAttribute Name="Widths" UniqueId="f7a25a3c-dada-4cd8-87fa-d2e2b0e3f986" DefinedType="IfcNonNegativeLengthMeasure" AggregationType="1" AggregationLower="1" AggregationUpper="?">
			<Documentation>The horizontal widths (when HorizontalWidths=.T.) or distances along the Slope (when HorizontalWidths=.F.) for the segments in the profile. And if Horizontal=.T. the Slopes shall not be = +/-90 deg.</Documentation>
		</DocAttribute>
		<DocAttribute Name="IsWidthHorizontal" UniqueId="4a33711f-47e1-49ef-94d8-502fe70bd714" DefinedType="IfcBoolean">
			<Documentation>Indicates if the widths shallbe measured horizontally or along the slopes.</Documentation>
		</DocAttribute>
		<DocAttribute Name="Slopes" UniqueId="9f8b24fc-ca73-4829-8d5e-c243faa2bc81" DefinedType="IfcPlaneAngleMeasure" AggregationType="1" AggregationLower="1" AggregationUpper="?">
			<Documentation>Slopes of segments.</Documentation>
		</DocAttribute>
		<DocAttribute Name="Tags" UniqueId="aeddf998-dc13-493b-af2d-8e1b5b6b6f04" DefinedType="IfcLabel" AttributeFlags="1" AggregationType="1" AggregationLower="2" AggregationUpper="?">
			<Documentation>Label identifying each cross section point of the profile definition. (Tags should unambiguously identify each longitudinal breakline within SectionedSurface).</Documentation>
		</DocAttribute>
	</Attributes>
	<WhereRules>
		<DocWhereRule Name="CorrectProfileType" UniqueId="10dd9554-1c37-4ebe-8fef-7ef0951568dd">
			<Expression>SELF\IfcProfileDef.ProfileType = IfcProfileTypeEnum.CURVE</Expression>
		</DocWhereRule>
		<DocWhereRule Name="CorrespondingSlopeWidths" UniqueId="e0d839ff-187e-49d5-ad0e-e3be90e8f21d">
			<Expression>SIZEOF(Slopes) = SIZEOF(Widths)</Expression>
		</DocWhereRule>
		<DocWhereRule Name="CorrespondingTags" UniqueId="5c8937a2-2cb8-4ca9-8723-b18b4439bd1e">
			<Expression>(NOT EXISTS (Tags)) OR (SIZEOF(Tags) = (SIZEOF(Slopes) + 1))</Expression>
		</DocWhereRule>
	</WhereRules>
</DocEntity>

